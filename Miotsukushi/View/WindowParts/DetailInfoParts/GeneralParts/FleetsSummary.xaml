<UserControl x:Class="Miotsukushi.View.WindowParts.DetailInfoParts.GeneralParts.FleetsSummary"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Miotsukushi.View.WindowParts.DetailInfoParts.GeneralParts"
             mc:Ignorable="d" 
             d:DesignHeight="700" d:DesignWidth="554">
    <UniformGrid Columns="4" Rows="1">
        <local:FleetSummary DataContext="{Binding FleetSummaryViewModel[0]}"/>
        <local:FleetSummary DataContext="{Binding FleetSummaryViewModel[1]}"/>
        <local:FleetSummary DataContext="{Binding FleetSummaryViewModel[2]}"/>
        <local:FleetSummary DataContext="{Binding FleetSummaryViewModel[3]}"/>
    </UniformGrid>
</UserControl>
